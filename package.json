{
  "name": "heinicus-mobile-mechanic-app",
  "main": "expo-router/entry",
  "version": "1.1.0",
  "scripts": {
    "start": "expo start",
    "start-web": "expo start --web",
    "start-tunnel": "expo start --tunnel",
    "start-web-dev": "DEBUG=expo* expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo build",
    "build-web": "expo build:web",
    "build-android": "expo build:android",
    "build-ios": "expo build:ios",
    "export": "expo export",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:verbose": "jest --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prebuild": "npm run build:status",
    "prebuild:clean": "expo prebuild --clean",
    "bump": "node scripts/bumpVersion.js",
    "bump:patch": "node scripts/bumpVersion.js patch",
    "bump:minor": "node scripts/bumpVersion.js minor",
    "bump:major": "node scripts/bumpVersion.js major",
    "bump:build": "node scripts/bumpVersion.js build",
    "build:dev": "npm run bump:build && eas build -p android --profile development --non-interactive",
    "build:preview": "npm run bump:patch && eas build -p android --profile preview --non-interactive",
    "build:prod": "npm run bump:minor && eas build -p android --profile production --non-interactive",
    "build:apk": "npm run build:standalone",
    "build:standalone": "npm run bump:build && eas build -p android --profile standalone --non-interactive",
    "build:local": "npm run bump:build && eas build -p android --profile development --local --non-interactive",
    "build:quick": "npm run bump:build && eas build -p android --profile development --non-interactive --clear-cache",
    "build:status": "node scripts/check-build-status.js",
    "test:ai": "jest --testPathPattern=ai",
    "postinstall": "npx prisma generate"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hono/trpc-server": "^0.3.4",
    "@prisma/client": "^5.22.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@stripe/stripe-react-native": "^0.45.0",
    "@tanstack/react-query": "^5.80.7",
    "@trpc/client": "^11.4.1",
    "@trpc/react-query": "^11.4.1",
    "@trpc/server": "^11.4.1",
    "ajv": "^8.17.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "expo": "^53.0.19",
    "expo-blur": "~14.1.4",
    "expo-camera": "~16.1.10",
    "expo-constants": "~17.1.4",
    "expo-device": "^7.1.4",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-location": "~18.1.4",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.6",
    "expo-web-browser": "~14.2.0",
    "firebase-admin": "^12.7.0",
    "hono": "^4.7.11",
    "jsonwebtoken": "^9.0.2",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "nodemailer": "^6.9.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "superjson": "^2.2.2",
    "ws": "^8.18.3",
    "zod": "^3.25.64",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.72.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.50.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "prisma": "^5.22.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
