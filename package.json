{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.1.0",
  "scripts": {
    "start": "bunx rork start -p g0jj0g95sgl0ixkxuef89 --tunnel",
    "start-web": "bunx rork start -p g0jj0g95sgl0ixkxuef89 --web --tunnel",
    "start-web-dev": "DEBUG=expo* bunx rork start -p g0jj0g95sgl0ixkxuef89 --web --tunnel",
    "start-tunnel": "expo start --tunnel",
    "websocket": "npx tsx backend/websocket/index.ts",
    "android": "bunx rork start --android",
    "ios": "bunx rork start --ios",
    "web": "bunx rork start --web",
    "build": "expo build",
    "build-web": "expo build:web",
    "build-android": "expo build:android",
    "build-ios": "expo build:ios",
    "export": "expo export",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:verbose": "jest --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ai": "jest --testPathPattern=ai",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android:dev": "eas build --platform android --profile development --local",
    "build:android:preview": "eas build --platform android --profile preview --local",
    "build:android:production": "eas build --platform android --profile production",
    "build:android:apk": "eas build --platform android --profile standalone --local",
    "submit:android": "eas submit --platform android",
    "prebuild": "npm run build:status",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:android": "bunx expo prebuild --platform android --clean",
    "install:android": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "bump": "node scripts/bumpVersion.js",
    "bump:patch": "node scripts/bumpVersion.js patch",
    "bump:minor": "node scripts/bumpVersion.js minor",
    "bump:major": "node scripts/bumpVersion.js major",
    "bump:build": "node scripts/bumpVersion.js build",
    "build:dev": "npm run bump:build && eas build -p android --profile development --non-interactive",
    "build:preview": "npm run bump:patch && eas build -p android --profile preview --non-interactive",
    "build:prod": "npm run bump:minor && eas build -p android --profile production --non-interactive",
    "build:apk": "npm run build:standalone",
    "build:standalone": "npm run bump:build && eas build -p android --profile standalone --non-interactive",
    "build:local": "npm run bump:build && eas build -p android --profile development --local --non-interactive",
    "build:quick": "npm run bump:build && eas build -p android --profile development --non-interactive --clear-cache",
    "build:status": "node scripts/check-build-status.js",
    "generate-lock": "chmod +x scripts/generate-package-lock.sh && ./scripts/generate-package-lock.sh",
    "build:android": "chmod +x scripts/build-android.sh && ./scripts/build-android.sh",
    "setup:ci": "chmod +x scripts/setup-ci.sh && ./scripts/setup-ci.sh",
    "postinstall": "npx prisma generate"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@expo/cli": "^0.24.18",
    "@expo/vector-icons": "^15.0.3",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@hono/trpc-server": "^0.4.0",
    "@hono/zod-validator": "^0.7.4",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@stripe/stripe-react-native": "^0.57.0",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "ajv": "^8.17.1",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "expo": "~54.0.0",
    "expo-application": "~6.1.5",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.10",
    "expo-device": "^8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.14",
    "expo-server-sdk": "^4.0.0",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "firebase-admin": "^13.5.0",
    "hono": "^4.7.11",
    "jsonwebtoken": "^9.0.2",
    "lucide-react-native": "^0.552.0",
    "nativewind": "^4.1.23",
    "nodemailer": "^7.0.10",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-maps": "^1.20.1",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^19.2.1",
    "superjson": "^2.2.2",
    "ws": "^8.18.3",
    "zod": "^3.25.64",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@prisma/client": "^6.19.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^7.0.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "~19.1.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^30.2.0",
    "jest-expo": "~54.0.0",
    "prisma": "^6.19.0",
    "react-test-renderer": "19.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react-test-renderer": "19.1.0"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "install": {
      "exclude": [
        "react-native-reanimated"
      ]
    }
  }
}
