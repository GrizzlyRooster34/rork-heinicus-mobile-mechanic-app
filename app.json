{
  "expo": {
    "name": "Heinicus Mechanic",
    "slug": "heinicus-mobile-mechanic-app",
    "version": "1.1.0",
    "owner": "heinicus1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobilemechanic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.heinicus.mobilemechanic"
    },
    "android": {
      "versionCode": 10,
      "package": "com.heinicus.mobilemechanic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CALL_PHONE",
        "SEND_SMS",
        "RECEIVE_SMS",
        "READ_SMS"
      ],
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "minSdkVersion": 21
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://mobilemechanic.app/"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#000000",
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "Mobile Mechanic",
      "iosDisplayInForeground": true
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "43815cbd-f56d-401c-8893-5c609bc71a27"
      },
      "buildInfo": {
        "buildNumber": 10,
        "buildDate": "2025-10-09T14:28:50.888Z",
        "gitBranch": "integrated-production",
        "gitCommit": "37cadf0",
        "buildType": "build",
        "environment": "development"
      }
    }
  }
}